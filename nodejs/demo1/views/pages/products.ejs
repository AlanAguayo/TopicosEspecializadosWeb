<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>Prueba de EJS</h1>
        <p>Products</p>
    </div>
</main>


<ul>
    <% products.forEach(function(product) { %>
        <li>
            <strong><%= product.name %> - </strong>
            Sale price: <%= product.sale_price %>        </li>
    <% }); %>
</ul>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>